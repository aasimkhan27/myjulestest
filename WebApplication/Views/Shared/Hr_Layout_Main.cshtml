    <div class="modal fade" id="leave">
        <form id="CustomForm" name="CustomForm" novalidate>
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">{{LEAVE_TEXT_HEADER}}</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body custom-scrollbar height-450">
                        <div class="row">
                            <div class="col-md-6" ng-show="REPORTING_EMPLOYEE_LIST.length>0">
                                <div class="form-group">
                                    <label>Apply Leave for:</label>
                                    <select class="custom-select"
                                            name="EMPLOYEE"
                                            ng-disabled="RECORD_LEAVE"
                                            ng-change="NG_CHANGE_LEAVE()"
                                            ng-model="LeaveSearch.REPORTING_EMPLOYEE_ID"
                                            ng-options="PT.EMP_PRS_ID as PT.USER_NAME  for PT in (REPORTING_EMPLOYEE_LIST)">
                                        <option value="" ng-if="EMPLOYEE_ID!='null'">Myself</option>
                                        <option value="" ng-if="EMPLOYEE_ID=='null'">Select</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Absence Type:*</label>
                                    <select class="custom-select"
                                            name="ABSENCE_TYPE"
                                            ng-model="LeaveSearch.ABSENCE_TYPE_ID"
                                            ng-options="PT.ABSENCE_TYPE_ID as PT.ABSENCE_TYPE_NAME  for PT in (ABSENCE_TYPE)"
                                            required
                                            ng-change="GET_REMAINING_LEAVE_EMP()"
                                            ng-class="{'error':CustomForm.submitted && CustomForm.ABSENCE_TYPE.$error.required}">
                                        <option value="">-Select Absence Type-</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="p-3 text-center mb-2" ng-class="{'bg-light-blue':LeaveSearch.ABSENCE_TYPE_ID!=null && REMAINING_DAYS.ENTITLEMENT_REQUIRED && REMAINING_DAYS.IS_LEAVE_ASIGNED,'bg-white':!(LeaveSearch.ABSENCE_TYPE_ID!=null && REMAINING_DAYS.ENTITLEMENT_REQUIRED && REMAINING_DAYS.IS_LEAVE_ASIGNED)}">
                                    <h5 class="heading-text">
                                        <span ng-show="LeaveSearch.ABSENCE_TYPE_ID!=null && REMAINING_DAYS.ENTITLEMENT_REQUIRED && REMAINING_DAYS.IS_LEAVE_ASIGNED">
                                            You have {{REMAINING_DAYS.BALANCE_ACCURED==null?REMAINING_DAYS.BALANCE_ACCURED=0:REMAINING_DAYS.BALANCE_ACCURED}}
                                            <span ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==1">days</span>
                                            <span ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==3">shift</span>
                                            <span ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==2">hours</span>  leave remaining
                                        </span>
                                    </h5>
                                </div>
                            </div>
                        </div>
                        <div class="row" ng-show="LeaveSearch.ABSENCE_TYPE_ID!=null">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Start Date:*</label>
                                    <div class="input-icons">
                                        <i class="fas fa-sort date-icon"></i>
                                        <input class="form-control input-i dateinputOpenDate"
                                               type="text"
                                               name="START_DATE"
                                               readonly
                                               placeholder="Select Date"
                                               ng-model="LeaveSearch.START_DATE"
                                               ng-value="LeaveSearch.START_DATE | date"
                                               required
                                               ng-change="SetStartCompleteDate(LeaveSearch.START_TIME, LeaveSearch.START_DATE, 1)"
                                               ng-class="{'error':CustomForm.submitted && CustomForm.START_DATE.$error.required || !START_DATE_REQUIRED_VALID}">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>End Date:*</label>
                                    <div class="input-icons">
                                        <i class="fas fa-sort date-icon"></i>
                                        <input class="form-control input-i dateinputOpenDate"
                                               type="text"
                                               name="END_DATE"
                                               placeholder="Select Date"
                                               ng-model="LeaveSearch.END_DATE"
                                               ng-value="LeaveSearch.END_DATE | date"
                                               readonly
                                               required
                                               ng-change="SetEndCompleteDate(LeaveSearch.END_TIME, LeaveSearch.END_DATE, 2)"
                                               ng-class="{'error':CustomForm.submitted && CustomForm.END_DATE.$error.required  || !END_DATE_REQUIRED_VALID}">
                                        <!--ng-class="{'error':CustomForm.submitted && CustomForm.END_DATE.$error.required || || !END_DATE_REQUIRED_VALID}"-->
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Start Time: <span ng-show="LeaveSearch.START_TIME_VALID">*</span> </label>
                                    <input onkeypress="return event.charCode >= 48 && event.charCode <= 58" class="form-control"
                                           placeholder="Select a time"
                                           maxlength="5"
                                           name="START_TIME"
                                           moment-picker="LeaveSearch.START_TIME"
                                           locale="{{LANGUAGE_SYMBOL}}"
                                           autocomplete="off"
                                           format="HH:mm"
                                           ng-pattern="/^([01]?[0-9]|2[0-3]):[0-5][0-9]$/"
                                           ng-model="LeaveSearch.START_TIME"
                                           ng-model-options="{ updateOn: 'blur' }"
                                           show-header="0"
                                           ng-blur="ngstarttimechange()"
                                           change="SetMomentStartCompleteDate(3)"
                                           ng-required="LeaveSearch.START_TIME_VALID"
                                           ng-class="{'error':CustomForm.submitted && CustomForm.START_TIME.$error.required || !START_DATE_REQUIRED_VALID}">
                                    <small class="text-danger" ng-show="CustomForm.START_TIME.$error.pattern">Please enter valid start time</small>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>End Time:<span ng-show="LeaveSearch.END_TIME_VALID">*</span> </label>
                                    <input onkeypress="return event.charCode >= 48 && event.charCode <= 58" class="form-control"
                                           maxlength="5"
                                           placeholder="Select a time"
                                           name="END_TIME"
                                           moment-picker="LeaveSearch.END_TIME"
                                           ng-model="LeaveSearch.END_TIME"
                                           autocomplete="off"
                                           format="HH:mm"
                                           ng-pattern="/^([01]?[0-9]|2[0-3]):[0-5][0-9]$/"
                                           locale="{{LANGUAGE_SYMBOL}}"
                                           show-header="0"
                                           ng-blur="ngendtimechange()"
                                           ng-model-options="{ updateOn: 'blur' }"
                                           change="SetMomentEndCompleteDate(4)"
                                           ng-required="LeaveSearch.END_TIME_VALID"
                                           ng-class="{'error':CustomForm.submitted && CustomForm.END_TIME.$error.required || !END_DATE_VALID || !END_DATE_REQUIRED_VALID}">
                                    <small class="text-danger" ng-show="CustomForm.END_TIME.$error.pattern">Please enter valid end time</small>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <label>Duration:*</label>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <input type="text"
                                           class="form-control"
                                           name="CAL_DAY_OR_HOURES"
                                           only-digits
                                           maxlength="5"
                                           required
                                           ng-class="{'error':CustomForm.submitted && CustomForm.CAL_DAY_OR_HOURES.$error.required}"
                                           ng-model="LeaveSearch.CAL_DAY_OR_HOURES">
                                </div>
                            </div>
                            <div class="col-md-1 mt-2" ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==1">Days</div>
                            <div class="col-md-1 mt-2" ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==2">Hours</div>
                            <div class="col-md-1 mt-2" ng-show="REMAINING_DAYS.ENTITLEMENT_UNIT_ID==3">Shifts</div>
                            <div class="col-md-2" ng-show="false">
