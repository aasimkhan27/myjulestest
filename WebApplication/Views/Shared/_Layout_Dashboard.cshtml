<body ng-controller="MasterController" ng-init="AppVersion=18+@System.Configuration.ConfigurationManager.AppSettings["AppVersion"].ToString()" ng-cloak bn-document-click="handleClick($event)"
      class="sidebar-mini layout-fixed layout-navbar-fixed sidebar-collapse custom-scrollbar">
    <div class="overlay_loadingNew" style="display:{{overlay_loadingNew}}!important"></div>
    <div class="overlay_loading" style="display:{{overlay_loading}}!important"></div>
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light-old">
            <a>
                <img ng-show="LOGO_PATH=='' || LOGO_PATH=='null'" src="~/E_Content/images/logo.png" class="brand-image" style="min-width:150px;margin-left:15px;" />
                <img ng-show="LOGO_PATH!='' && LOGO_PATH!='null'" src="{{LOGO_PATH}}" class="brand-image" style="min-width:150px;margin-left:15px;" />
            </a>
            <header>
                <nav id='cssmenu'>
                    <div id="head-mobile"></div>
                    <div class="top_nav_button"></div>
                    <ul>
                        <li class="has-sub1" ng-click="MENU_CLICK(1,'DLR')" ng-class="{'active':'DLR'==SELECTED_TAB_NAME}">
                            <a href="../DashBoard/DashboardIndex#!/DRL"><i class="fal fa-home fa-lg" ng-show="false"></i>Revenue Dashboard</a>
                            <ul>
                                <li ng-click="MENU_CLICK(2,'DLDA')" ng-class="{'active':'DLDA'==SELECTED_TAB_NAME}">
                                    <a href='../DashBoard/DashboardIndex#!/DRDA'><i class="fal fa-users" ng-show="false"></i> Revenue Details</a>
                                </li>
                                <li ng-click="MENU_CLICK(4,'NSS')" ng-class="{'active':'NSS'==SELECTED_TAB_NAME}">
                                    <a href='../DashBoard/DashboardIndex#!/NSS'><i class="fal fa-users" ng-show="false"></i> Net Sales</a>
                                </li>
                                <li ng-click="MENU_CLICK(5,'TR')" ng-class="{'active':'TR'==SELECTED_TAB_NAME}">
                                    <a href='../DashBoard/DashboardIndex#!/TR'><i class="fal fa-users" ng-show="false"></i> Takings Review</a>
                                </li>
                                <li ng-click="MENU_CLICK(5,'Consolidated_Revenue_Dashboard')" ng-class="{'active':'Consolidated_Revenue_Dashboard'==SELECTED_TAB_NAME}">
                                    <a href="../DashBoard/DashboardIndex#!/Consolidated_Revenue_Dashboard"><i class="fal fa-users" ng-show="false"></i> Consolidated Sales</a>
                                </li>
                            </ul>
                        </li>
                        <li class="has-sub1" ng-click="MENU_CLICK(5,'Cogs')" ng-class="{'active':'Consolidated_Revenue_Dashboard'==SELECTED_TAB_NAME}">
                            <a href="../DashBoard/DashboardIndex#!/Cogs" ng-click="MENU_CLICK(3,'Cogs')">COGS</a>
                            <ul>
                                <li ng-click="MENU_CLICK(5,'Purchases')" ng-class="{'active':'Purchases'==SELECTED_TAB_NAME}">
                                    <a href="../DashBoard/DashboardIndex#!/Purchases">Purchases</a>
                                </li>
                                <li ng-click="MENU_CLICK(5,'MP')" ng-class="{'active':'Menu_Profitability'==SELECTED_TAB_NAME}">
                                    <a href="../DashBoard/DashboardIndex#!/MP">Menu Profitability</a>
                                </li>
                            </ul>
                        </li>
                        <li class="has-sub1" ng-class="{'active':'PR'==SELECTED_TAB_NAME}">
                            <a href="../DashBoard/DashboardIndex#!/PR" ng-click="MENU_CLICK(3,'PR')">Payroll Dashboard</a>
                        </li>
                        <li class="has-sub1" ng-class="{'active':'PNL_DASBOARD_UPLOAD'==SELECTED_TAB_NAME}">
                            <a>Upload</a>
                            <ul>
                                <li ng-if="CheckSubModuleAccess(124)" ng-click="MENU_CLICK(5,'PNL_DASBOARD_UPLOAD')" ng-class="{'active':'PNL_DASBOARD_UPLOAD'==SELECTED_TAB_NAME}">
                                    <a href="../DashBoard/DashboardIndex#!/PNL_Dasboard_Upload">P&L</a>
                                </li>
                            </ul>
                        </li>
                        @*<ul>
                                <li ng-click="MENU_CLICK(3,'PR')" ng-class="{'active':'PR'==SELECTED_TAB_NAME}">
                                    <a href='../DashBoard/DashboardIndex#!/PR'>
                                        <i class="fal fa-users" ng-show="false"></i> Payroll Reports
                                    </a>
                                </li>
                            </ul>*@


                    </ul>
                </nav>
            </header>
            <ul class="navbar-nav ml-auto mr-3">
                <li ng-show="false" class="nav-item dropdown text-sm mt-2">Become as <span class="text-blue semibold ml-1">Myself</span></li>
                @*<li class="nav-item dropdown"> <span class="nav-link text-sm cursor-default">{{SELECTED_ENTITY_NAME}}</span></li>*@
                <li class="nav-item dropdown">
                    <a class="nav-link app-text" data-toggle="dropdown">
                        <i class="fas fa-th fa-lg"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-app dropdown-menu-right dropdown-menu-arrow">
                        <div class="card mb-0">
                            <div class="card-body custom-scrollbar" style="max-height:80vh">
                                <div class="at-grid bg-white" data-column="4">
                                    <div class="at-column app-suit-inside cursor-pointer {{Module.CLASS_NAME}}" ng-repeat="Module in ModuleList" ng-click="ModuleRedirection(Module)"
                                         ng-if="Module.IS_VISIBLE_IN_APP_SUITE">
                                        <div class="at-user">
                                            <div class="at-user__avatar inside_icons"><img src="../E_Content/images/Appsuite_Icons/{{Module.MODULE_ICON | trim }}" /></div>
                                            <div class="text-white font-8">{{Module.MODULE_NAME}}</div>
                                        </div>
                                    </div>
                                </div>
                                <a href="../Main/MainIndex" class="btn btn-wenodo btn-block btn-sm mt-2">Back to App Suite</a>
                            </div>
                        </div>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link app-text text-sm pt-0" data-toggle="dropdown">
                        <img ng-show="IMAGE_PATH=='' || IMAGE_PATH=='null'" src="../dist/img/user1-128x128.jpg" class="img-size-32 mt-2 mr-4 img-circle" />
                        <img ng-show="IMAGE_PATH!='' && IMAGE_PATH!='null'" src="{{IMAGE_PATH}}" class="img-size-32 mt-2 mr-4 img-circle" />
                    </a>
                    <ul class="dropdown-menu dropdown-menu-lg dropdown-menu-right dropdown-menu-arrow">
                        <li class="user-header">
                            <ul class="list-line my-1">
                                <li class="list-line-item cursor-default hover-none"><i class="fas fa-user"></i> {{User_Name}}</li>
                                <li class="list-line-item cursor-default hover-none"><i class="fas fa-envelope"></i> {{User_Email}}</li>
                            </ul>
                        </li>
                        <li class="user-footer">
                            <div class="row">
                                <div class="col-md-6 text-center"><a href="../AccountSetting/index#!/BasicInfo" type="button" class="btn btn-w-default btn-sm"> Settings</a></div>
                                <div class="col-md-6 text-center"><button type="button" class="btn btn-wenodo btn-sm" title="Logout" ng-click="INS_LOGOUT_LOG(1)">Sign out</button></div>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <div class="content-wrapper">
            <section class="content">
                @RenderBody()
            </section>
        </div>
        <footer class="hr-dashboard-main-footer">
            <div class="row">
                <div class="col app-text font-14 d-flex pt-1">
                    <span class="mt-2 ml-4 font-12">Powered by </span>
                    <a target="_blank" href="http://wenodo.com/"><img src="~/E_Content/images/logo.png" class="img-fluid footer-logo" /></a>
                </div>
                <div class="col-auto" ng-show="false">
                    <div class="weather-wrapper">
                        <div class="weather-card madrid">
                            <div class="weather-icon sun" ng-class="cloud"></div>
                            <h1>26º <span>Madrid</span></h1>
                        </div>
                    </div>
                </div>
                <div class="col-auto text-right">
                    <div class="footer-links mt-1">
                        <a ng-show="false">Cookies</a>
                        <a href="https://wenodo.com/policy/" target="_blank">Privacy Policy</a>
                        <a href="https://wenodo.com/client-contact-us/" target="_blank">Contact</a>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <div id="return-to-top"><i class="fa fa-arrow-up" aria-hidden="true"></i></div>

    <script src="~/E_Content/plugins/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/AngularControllers/RouteProvider.js?@System.Configuration.ConfigurationManager.AppSettings["AppVersion"].ToString()"></script>
    <script src="~/AngularControllers/Angular_Directive.js?@System.Configuration.ConfigurationManager.AppSettings["AppVersion"].ToString()"></script>
    <script src="~/AngularControllers/MasterCntrl.js?@System.Configuration.ConfigurationManager.AppSettings["AppVersion"].ToString()"></script>

    <script src="~/E_Scripts/angular-sanitize.min.js"></script>
    <script src="~/E_Content/plugins/select2/js/select2.full.min.js"></script>
    <script src="~/E_Scripts/JsontoCSV/ng-csv.js"></script>

    <script src="~/E_Scripts/Fusion_chart/js/fusioncharts.js"></script>
    <script src="~/E_Scripts/Fusion_chart/js/themes/fusioncharts.theme.fusion.js"></script>
    <script src="~/E_Scripts/Fusion_chart/js/fusioncharts.charts.js"></script>
    <script src="~/E_Scripts/Fusion_chart/integrations/angularjs/js/angular-fusioncharts.min.js"></script>
    <script type="text/javascript" src=" https://cdn.fusioncharts.com/fusioncharts/latest/fusioncharts.timeseries.js"></script>

    <script src="~/E_Scripts/ExcelPdfJS/jspdf.min.js"></script>



    <script src="~/E_Scripts/ExcelPdfJS/alasql.min.js"></script>
    <script src="~/E_Scripts/ExcelPdfJS/xlsx.core.min.js"></script>

    <script src="~/E_Scripts/MomentTimePicker/moment-with-locales.min.js"></script>
    <script src="~/E_Scripts/MomentTimePicker/angular-moment-picker.min.js"></script>
    <link href="~/E_Scripts/MomentTimePicker/angular-moment-picker.min.css" rel="stylesheet" />
    <link href="~/E_Scripts/daterangepicker-master/daterangepicker.css" rel="stylesheet" />
    <script src="~/E_Scripts/daterangepicker-master/daterangepicker.js"></script>
