<div class="modal fade" tabindex="-1" id="History" role="dialog" aria-labelledby="myExtraLargeModal" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myExtraLargeModal">History</h4>
                <button class="btn-close py-0" type="button" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body p-0">
                <div class="row">
                    <div class="col-xxl-12 col-md-12 col-xl-12">
                        <table>
                            <thead>
                                <tr>
                                    <th>Data Updated</th>
                                    <th>Modified Date</th>
                                    <th>Updated by</th>
                                    <th>Comment</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="_history in EMPLOYEE_HISTORY_HEADERS_LIST">
                                <tr>
                                    <td>{{_history.STEP_NAME}}</td>
                                    <td>{{_history.ARCHIVE_DATE | date}}</td>
                                    <td>{{_history.USER_NAME}}</td>
                                    <td>{{_history.COMMENTS==null || _history.COMMENTS==''?'-':_history.COMMENTS}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>