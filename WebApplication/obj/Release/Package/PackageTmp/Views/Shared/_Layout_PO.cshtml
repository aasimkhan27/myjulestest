    <div class="modal fade show" id="View_Invoice">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" ng-show="false">Invoice {{INVOICES_RESULT.INVOICENUMBER}}</h4>
                    <h4 class="modal-title font-16">Bill - {{INVOICES_RESULT.INVOICENUMBER}}</h4>

                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body custom-scrollbar">
                    <div class="modal-header pl-0 pt-0">
                        <span class="badge badge-success mt-1"> {{INVOICES_RESULT.STATUS}}</span>
                        <div class="card-tools">
                            <ul class="nav nav-pills ml-auto">
                                @*<li class="nav-item mr-2">
                                        <button type="button" class="btn btn-sm btn-wenodo" data-toggle="tootltip" data-html="true" data-original-title="Print PDF">Print PDF</button>
                                    </li>*@
                                <li class="nav-item" ng-show="INVOICES_RESULT.ATTACHMENTS.length==0">
                                    - No Attachments
                                </li>
                                <li class="nav-item" ng-show="INVOICES_RESULT.ATTACHMENTS.length>0">
